{
"compilerOptions": {
  "strict": false,  // 완전 풀어버리기
  "esModuleInterop": true,
  "resolveJsonModule": true,
  "allowSyntheticDefaultImports": true,
  "allowJs": true,
  "module": "ESNext",
  "target": "ES2020",
  "baseUrl": ".",
  "paths": {
    "@assets/*": ["./src/assets/*"],
    "@components/*": ["./src/components/*"],
    "@lib/*": ["./src/lib/*"],
    "@pages/*": ["./src/pages/*"],
    "@router/*": ["./src/router/*"],
    "@services/*": ["./src/services/*"],
    "@stores/*": ["./src/stores/*"]
  },
  "typeRoots": ["./node_modules/@types", "./src/types"]
}
}
